# Server Configuration
PORT=3001
SERVER_URL=http://localhost:3001
FRONTEND_URL=http://localhost:5173

# JWT Secret (change this in production!)
JWT_SECRET=codesync-super-secret-jwt-key-change-me

# GitHub OAuth
# Create app at: https://github.com/settings/developers
# Set callback URL to: http://localhost:3001/auth/github/callback
GITHUB_CLIENT_ID=your_github_client_id_here
GITHUB_CLIENT_SECRET=your_github_client_secret_here

# Google OAuth  
# Create app at: https://console.cloud.google.com/apis/credentials
# Set callback URL to: http://localhost:3001/auth/google/callback
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here
